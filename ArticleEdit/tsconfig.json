{
  "baseUrl": ".",
  "path": {    
    "qunit": "Scripts/qunit-2.19.3"
  },
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "outDir": "Scripts/tsJs",
    "module": "AMD"
  },
  "include": [
    "./Scripts/ts/**/*",
    "./Scripts/typings/**/*",
    "./Scripts"
  ],
  //"Scripts/tsJs/DocumentReady.js", "Scripts/tsJs/Tests.js"  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true
}
